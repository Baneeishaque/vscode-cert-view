{
  "name": "cert-view",
  "displayName": "Certificate View",
  "description": "View the content of certificate files",
  "version": "0.0.1",
  "publisher": "CPlusSharp",
  "author": {
    "name": "CPlusSharp",
    "email": "cgraefe83@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cplussharp/vscode-cert-view/issues",
    "email": "cgraefe83@gmail.com"
  },
  "homepage": "https://github.com/cplussharp/vscode-cert-view",
  "repository": {
    "type": "git",
    "url": "https://github.com/cplussharp/vscode-cert-view.git"
  },
  "engines": {
    "vscode": "^1.55.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "X.509",
    "Certificate",
    "PEM",
    "PKCS#7",
    "ASN.1"
  ],
  "activationEvents": [
    "onCommand:cert-view.pemCertPreview",
    "onCommand:cert-view.pemAsn1Preview",
    "onLanguage:pem"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "pem",
        "aliases": [
          "PEM",
          "X509",
          "cert"
        ],
        "extensions": [
          ".pem",
          ".crt",
          ".cer",
          ".cert",
          ".csr",
          ".p7b"
        ],
        "firstLine": "^-----BEGIN ([A-Z ]+)-----"
      }
    ],
    "semanticTokenScopes": [
      {
        "scopes": {
          "language": [
            "pem"
          ],
          "operator": [
            "storage.type"
          ],
          "keyword": [
            "keyword.control"
          ],
          "type": [
            "entity.name.type"
          ]
        }
      }
    ],
    "commands": [
      {
        "command": "cert-view.pemCertPreview",
        "title": "Show PEM content",
        "category": "Cert View",
        "icon": "$(preview)"
      },
      {
        "command": "cert-view.pemAsn1Preview",
        "title": "Show PEM content as ASN.1",
        "category": "Cert View",
        "icon": "$(file-code)"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceLangId == pem",
          "command": "cert-view.pemCertPreview",
          "group": "navigation"
        },
        {
          "when": "resourceLangId == pem",
          "command": "cert-view.pemAsn1Preview",
          "group": "navigation"
        }
      ],
      "editor/title": [
        {
          "when": "editorLangId == pem",
          "command": "cert-view.pemCertPreview",
          "group": "navigation"
        },
        {
          "when": "editorLangId == pem",
          "command": "cert-view.pemAsn1Preview",
          "group": "navigation"
        }
      ],
      "editor/title/context": [
        {
          "when": "editorLangId == pem",
          "command": "cert-view.pemCertPreview",
          "group": "navigation"
        },
        {
          "when": "editorLangId == pem",
          "command": "cert-view.pemAsn1Preview",
          "group": "navigation"
        }
      ],
      "commandPalette": [
        {
          "when": "editorLangId == pem",
          "command": "cert-view.pemCertPreview",
          "group": "navigation"
        },
        {
          "when": "editorLangId == pem",
          "command": "cert-view.pemAsn1Preview",
          "group": "navigation"
        }
      ]
    },
    "keybindings": [
      {
        "when": "editorLangId == pem",
        "command": "cert-view.pemCertPreview",
        "key": "shift+ctrl+v",
        "mac": "shift+cmd+v"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/asn1js": "^2.0.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/pkijs": "^0.0.6",
    "@types/vscode": "^1.55.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.4.0"
  },
  "dependencies": {
    "pkijs": "^2.1.94"
  }
}
